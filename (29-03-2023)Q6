#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5, 6, 7, 8, 9};
    int size = sizeof(arr) / sizeof(arr[0]);
    int even[size], odd[size], e = 0, o = 0;

    for (int i = 0; i < size; i++) {
        if (arr[i] % 2 == 0) {
            even[e++] = arr[i];
        } else {
            odd[o++] = arr[i];
        }
    }

    printf("Even Elements: ");
    for (int i = 0; i < e; i++) {
        printf("%d ", even[i]);
    }

    printf("\nOdd Elements: ");
    for (int i = 0; i < o; i++) {
        printf("%d ", odd[i]);
    }

    return 0;
}
